<launch>

        <node pkg="bumblebee2" type="bumblebee2" name="bumblebee2" output="screen" respawn="false">
                <param name="video_mode" value="format7_3" />
                <param name="fps" value="1" />
                <param name="gain" value="4305" />
                <param name="brightness" value="4096" />
                <param name="whitebalance" value="auto" />
                <param name="shutter" value="256" />
                <param name="exposure" value="555" />
                <param name="bayer_pattern" value="none" />
                <param name="bayer_method" value="none" />
        </node>


	
