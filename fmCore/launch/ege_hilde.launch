<launch>
  <group ns="kbhit">
    <node pkg="fmHMI" name="kbhit_node" type="kbhit_move" output="screen">
	<param name="topic" value="kbhit_topic" />
	<param name="twist_topic" value="/robocard/twist_topic" />
	<param name="max_speed" value="100" />
	<param name="min_speed" value="-100" />
	<param name="turn_constant" value="3" />
	<param name="ramp" value="20" />
    </node>
  </group>
  <group ns="robocard">
    <node pkg="fmPSP" type="hilde_ctrl_node" name="hilde_controller" output="screen">
	<param name="twist_subscriber_topic" value="twist_topic" type="string"/>
	<param name="serial_subscriber_topic" value="S0_rx_topic" type="string"/>
	<param name="serial_publisher_topic" value="S0_tx_topic" type="string"/>
	<param name="max_linear_velocity" value="1.0" type="double"/>
	<param name="bias_linear_velocity" value="1.0" type="double"/>
	<param name="bias_linear_velocity_percentage" value="0.8" type="double"/>	
	<param name="base_link_radius_to_wheels" value="0.185" type="double"/>
	<param name="base_link_length_to_rear_wheel" value="0.28" type="double"/>
    </node>
    <node pkg="fmBSP" name="serial_node" type="serial_node" output="screen">
	<param name="publisher_topic" value="S0_rx_topic" />
	<param name="subscriber_topic" value="S0_tx_topic" />
	<param name="device" value="/dev/ttyUSB0" />
	<param name="baudrate" value="115200" />
	<param name="bytes" value="2" />
    </node>
  </group>
</launch>
